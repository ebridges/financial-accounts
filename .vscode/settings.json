{
    "files.exclude": {
        "db": true,
        ".vscode": true,
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "editor.formatOnSave": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length",
        "100",
        "--skip-string-normalization"
    ],
    "editor.defaultFormatter": "ms-python.black-formatter",
    "flake8.args": ["--max-line-length=100", "--extend-ignore=E203,E266,E501,W503,E402,E902"],
    "black.args": ["line-length=100", "skip-string-normalization=true"],
  }
